  ],
  "license": "MIT",
  "dependencies": {
    "crypto-js": "^4.2.0",
    "debug": "4.3.7",
    "dotenv": "^16.4.5",
    "node-fetch": "^3.3.2",
    "superagent": "^10.1.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.9.1",
    "better-assert": "1.0.2",
    "istanbul": "^0.4.5",
    "mocha": "^10.8.2",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
